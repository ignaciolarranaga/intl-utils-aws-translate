{
  "name": "intl-utils-aws-translate",
  "version": "1.0.1",
  "description": "Helps to automatically translate files for intl-utils using AWS Translate",
  "main": "dist/index.js",
  "bin": {
    "intl-utils-aws-translate": "bin/translate.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "build": "tsc",
    "start": "ts-node src/index",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignaciolarranaga/intl-utils-aws-translate.git"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "translation",
    "automatic",
    "aws",
    "aws-translate"
  ],
  "author": "Ignacio Larra√±aga <ignaciolarranaga@gmail.com>",
  "license": "MIT",
  "files": [
    "package.json",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "bugs": {
    "url": "https://github.com/ignaciolarranaga/intl-utils-aws-translate/issues"
  },
  "homepage": "https://github.com/ignaciolarranaga/intl-utils-aws-translate#readme",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-translate": "^3.301.0",
    "commander": "^10.0.0",
    "glob": "^9.3.2"
  }
}
